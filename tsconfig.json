{
  "compilerOptions": {
    "target": "esnext",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": false,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "baseUrl": "./src",
  },
  "paths": {
    "components/index": [
      "./components/index"
    ],
    "data/index": [
      "./data/index"
    ],
    "store": [
      "./store.ts"
    ],
    "environment": [
      "./environment.ts"
    ],
  },
  "include": [
    "src"
  ]
}
